{
    "manifest_version": 2,

    "name": "AWS Linker",
    "description": "Improvements to AWS SDK Console",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "options_page": "options.html",

    "permissions": [
        "activeTab",
        "https://console.aws.amazon.com/",
        "https://*.amazonaws.com/"
    ]
}
